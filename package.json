{
  "name": "downloaded-csv-to-clipboard",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cd dist && zip -r ../downloaded_csv_to_clipboard.zip * && cd -",
    "test": "eslint-config-prettier src/**/*.ts src/**/*.tsx && prettier --check . && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && vitest run",
    "preview": "vite preview"
  },
  "dependencies": {
    "chardet": "^2.0.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.3",
    "@types/chrome": "^0.0.328",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  }
}
